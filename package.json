  "description": "NHS RPySoc Conference 2025 - Deploying Python in production (Built with reveal.js)",
  "homepage": "https://github.com/amadeuspzs/nhs-rpysoc-2025",
  "main": "dist/reveal.js",
  "module": "dist/reveal.esm.js",
  "license": "MIT",
  "scripts": {
    "test": "gulp test",
    "start": "gulp serve",
    "dev": "gulp serve",
    "build": "gulp build",
    "package": "gulp package",
    "clean": "rm -rf dist node_modules package-lock.json"
  },
  "author": {
    "name": "Amadeus Stevenson",
    "note": "Presentation content and customization"
  },
  "contributors": [
    {
      "name": "Hakim El Hattab",
      "email": "hakim.elhattab@gmail.com",
      "web": "https://hakim.se",
      "note": "reveal.js framework"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/amadeuspzs/nhs-rpysoc-2025.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "reveal",
    "slides",
    "presentation",
    "nhs",
    "python",
    "containers",
    "kubernetes"
  ],
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/eslint-parser": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-plugin-transform-html-import-to-string": "^2.0.0",
    "core-js": "^3.38.0",
    "fitty": "^2.3.7",
    "glob": "^10.4.0",
    "globals": "^16.5.0",
    "gulp": "^5.0.0",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-connect": "^5.7.0",
    "gulp-eslint-new": "^2.3.0",
    "gulp-header": "^2.0.9",
    "gulp-tap": "^2.0.0",
    "gulp-zip": "^5.1.0",
    "highlight.js": "^11.10.0",
    "marked": "^14.1.0",
    "node-qunit-puppeteer": "^2.1.2",
    "picocolors": "^1.1.0",
    "qunit": "^2.22.0",
    "rollup": "^4.22.0",
    "sass": "^1.79.0",
    "through2": "^4.0.2",
    "yargs": "^17.7.2"
  },
  "browserslist": "> 2%, not dead",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "requireConfigFile": false
    },
    "globals": {
      "module": false,
      "console": false,
      "unescape": false,
      "define": false,
      "exports": false
    },
    "rules": {
      "curly": 0,
      "eqeqeq": 2,
      "wrap-iife": [
        2,
        "any"
      ],
      "no-use-before-define": [
        2,
        {
          "functions": false
        }
      ],
      "new-cap": 2,
      "no-caller": 2,
      "dot-notation": 0,
      "no-eq-null": 2,
      "no-unused-expressions": 0
    }
  }
}